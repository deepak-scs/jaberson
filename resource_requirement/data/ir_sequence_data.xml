<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for Resource Requirement -->
        <record id="seq_resource_requirement" model="ir.sequence">
            <field name="name">Resource Requirement</field>
            <field name="code">resource.requirement</field>
            <field name="prefix">RR</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Sequences for Job Requirement -->
        <record id="seq_job_requirement" model="ir.sequence">
            <field name="name">Job Requirement</field>
            <field name="code">job.requirement</field>
            <field name="prefix">JR</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

    </data>
</odoo>
